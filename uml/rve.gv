digraph RVE {

    node[shape=record, style=rounded]
    RVE[label = "{RVE}"]
    RVE2D[label = "{RVE2D}"]
    RVE3D[label = "{RVE3D}"]

    // generalizations
    edge[dir=back, arrowtail=empty, style=normal]
